{
  "name": "basic-project",
  "dockerComposeFile": [
    "selenium-hub-docker.yml"
  ],
  "service": "tests",
  "mounts": [
    "source=maven-cache,target=/root/.m2,type=volume"
  ],
  "features": {
    "ghcr.io/devcontainers/features/java:1": {
      "version": "11",
      "installMaven": "true"
    }
  },
  "customizations": {
    "jetbrains": {
      "backend": "IntelliJ"
    },
    "codespaces": {
      "openFiles": [
        "README.md"
      ]
    },
    "vscode": {
      "extensions": [
        "alphabotsec.vscode-eclipse-keybindings",
        "ms-vscode-remote.vscode-remote-extensionpack"
      ],
      "settings": {
        "maven.pomfile.prefetchEffectivePom": true
      }
    }
  }
}
